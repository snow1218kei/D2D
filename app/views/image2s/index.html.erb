<p id="notice"><%= notice %></p>

<h1>Image2s</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image2s.each do |image2| %>
      <tr>
        <td><%= image2.image %></td>
        <td><%= link_to 'Show', image2 %></td>
        <td><%= link_to 'Edit', edit_image2_path(image2) %></td>
        <td><%= link_to 'Destroy', image2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image2', new_image2_path %>
